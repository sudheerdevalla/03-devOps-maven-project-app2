pipeline {
    agent any
    tools {
        maven 'M2_HOME'
    }
    stages {
        stage('Git') {
            steps {
                git branch: 'main', url: 'https://github.com/sudheerdevalla/03-devOps-maven-project-app2.git'
            }
        }
        stage('Hello') {
            steps {
                sh 'mvn clean compile'
            }
        }
    }
}
